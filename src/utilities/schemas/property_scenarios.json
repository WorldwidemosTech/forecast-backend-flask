{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "property_scenarios",
    "type": "object",
    "description": "Property scenarios schema",
    "required": [
      "capital",
      "income",
      "expense"
    ],
    "properties": {
        "property_id": {
            "type": "string",
            "title": "Property ID",
            "description": "Property ID"
        },
        "user_id": {
            "type": "string",
            "title": "User ID",
            "description": "User ID"
        },
        "capital": {
            "type":"object",
            "title": "Capital section",
            "description": "Capital section",
            "properties": {
                "categorie_a": {
                    "type": "object",
                    "title": "Capital subsection",
                    "description": "Capital subsection",
                    "properties": {
                        "value": {
                            "type": "number"
                        }
                        
                    }
                },
            "additionalProperties": true
            }
        },
        "expense": {
            "type":"object",
            "title": "Expense section",
            "description": "Expense section",
            "properties": {
                "categorie_a": {
                    "type": "integer",
                    "title": "Expense subsection",
                    "description": "Expense subsection"
                },
            "additionalProperties": true
            }
        },
        "income": {
            "type":"object",
            "title": "Income section",
            "description": "Income section",
            "properties": {
                "categorie_a": {
                    "type": "integer",
                    "title": "Income subsection",
                    "description": "Income subsection"
                },
            "additionalProperties": true
            }
         },
    "additionalProperties": true
  }
}
  