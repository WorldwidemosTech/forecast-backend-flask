{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "property_information",
    "type": "object",
    "description": "Property Information schema",
    "required": [
      "capital",
      "income",
      "expense",
      "information",
      "property_id",
      "user_id"
    ],
    "properties": {
        "property_id": {
            "type": "string",
            "title": "Property ID",
            "description": "Property ID"
        },
        "user_id": {
            "type": "string",
            "title": "User ID",
            "description": "User ID"
        },
        "information":{
            "type":"object",
            "title": "Property information",
            "description": "Property information",
            "properties": {
                "number_of_units":{"type": "integer"},
                "total_sq_ft":{"type": "integer"},
                "scheduled_rent":{"type": "integer"},
                "actual_status":{"type": "string"},
                "avg_unit_sq_ft":{"type": "integer"},
                "avg_rent_per_unit":{"type": "integer"},
                "budget_year":{"type": "integer"},
                "date":{"type": "string"},
                "turnover":{"type": "integer"},
                "application_fee":{"type": "integer"},
                "starting_vacant_units":{"type": "integer"}
            }
        },
        "capital": {
            "type":"object",
            "title": "Capital section",
            "description": "Capital section",
            "minProperties": 1,
            "properties": {
            "additionalProperties": {
                "type": "object",
                "title": "Capital subsection",
                "minProperties": 1,
                "description": "Capital subsection",
                "additionalProperties": {
                    "type": "number"
                    }
                }
            }
        },
        "expense": {
            "type":"object",
            "title": "Capital section",
            "description": "Capital section",
            "minProperties": 1,
            "properties": {
            "additionalProperties": {
                "type": "object",
                "title": "Capital subsection",
                "minProperties": 1,
                "description": "Capital subsection",
                "additionalProperties": {
                    "type": "number"
                    }
                }
            }
        },
        "income": {
            "type":"object",
            "title": "Capital section",
            "description": "Capital section",
            "minProperties": 1,
            "properties": {
            "additionalProperties": {
                "type": "object",
                "title": "Capital subsection",
                "minProperties": 1,
                "description": "Capital subsection",
                "additionalProperties": {
                    "type": "number"
                    }
                }
            }
         },
    "additionalProperties": false
  }
}
  